<!doctype html>
<html lang="en">
  <head>
	  <title>Canvas/GDrive LTI</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
		<%= Rack::Csrf.metatag(env) %>
		<meta name="_root_path" content="<%= url_for '/' %>">
		<link rel="stylesheet" href="<%= url_for stylesheet_path('app') %>">
  </head>
  <body>
		<div class="container">
			<div>
				<%if @error %>
          <div class="alert alert-error"><%=@error%></div>
				<% end %>
				<%= yield %>
			</div>
		</div>
		<script src="<%= url_for javascript_path('app') %>"></script>
  </body>
</html>
