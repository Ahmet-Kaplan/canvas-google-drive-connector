<form method="post" class="content-items-form" action="<%= session[:return_url] %>">
  <input type="hidden" name="lti_message_type" value="ContentItemSelection"/>
  <input type="hidden" name="lti_version" value="LTI-1p0"/>
  <input type="hidden" name="content_items" value='<%= ContentItems.build(file, type).to_json %>'/>
  <button type="submit" class="action"><%= btn_label %></button>
</form>
